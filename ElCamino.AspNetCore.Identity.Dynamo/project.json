{
  "version": "3.0.0-alpha1",
  "title": "ElCamino.AspNet.Identity.Dynamo v2",
  "description": "DynamoDB Storage Provider for ASP.NET Core Identity 1.0 framework. Does NOT work with projects targeting the .NET 4.5+ framework.",
  "language": "en-US",
  "authors": [
    "David Melendez",
    "Paul Oliver"
  ],
  "packOptions": {
    "projectUrl": "https://github.com/c0achmcguirk/AspNetIdentity_DynamoDB",
    "tags": [
      "Identity",
      "DynamoDB",
      "AWS",
      "Amazon"
    ],
    "licenseUrl": "https://github.com/c0achmcguirk/AspNetIdentity_DynamoDB/blob/master/LICENSE.txt",
    "repository": {
      "type": "git",
      "url": "git://github.com/c0achmcguirk/AspNetIdentity_DynamoDB"
    }
  },
  "buildOptions": {
    "compile": [
      "**/*.cs"
    ]
  },
  "dependencies": {
    "AWSSDK.Core": "3.2.8-rc",
    "AWSSDK.DynamoDBv2": "3.2.8-rc",
    "Microsoft.AspNetCore.Identity": "1.0.0",
    "Microsoft.Extensions.DependencyInjection": "1.0.0",
    "NETStandard.Library": "1.6.0",
    "Newtonsoft.Json": "9.0.2-beta1",
    "System.Linq.Queryable": "4.0.1",
    "System.Xml.XDocument": "4.3.0"
  },
  "frameworks": {
    "netstandard1.3": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net451+win8"
      ],
      "buildOptions": {
        "define": [
          "netstandard"
        ]
      }
    }
  }
}